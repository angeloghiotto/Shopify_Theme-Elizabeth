/* august sale page */
.page-sale {

    .sale__wrapper {
        padding-top: 35px;
        @media (max-width: 1199px) {
            padding-top: 15px;
        }
    }

    .page__title {
        font-size: 36px;
        color: #363636;
        line-height: 1.5;
        font-weight: 600;
        text-align: center;
        margin-bottom: 50px;
        text-transform: none;
        font-family: 'Poppins', sans-serif;
        @media (min-width: 1200px) {
            max-width: 1100px;
            margin-left: auto;
            margin-right: auto;
        }
        @media (max-width: 1199px) {
            font-size: 30px;
            margin-bottom: 30px;
        }
        @media (max-width: 991px) {
            font-size: 26px;
            margin-bottom: 25px;
        }
        @media (max-width: 767px) {
          font-size: 24px;
        }
        @media (max-width: 575px) {
          font-size: 19.14px;
        }
        @media (max-width: 375px) {
          font-size: 17.14px;
        }
    }

    .featurebox__wrapper {
        margin-bottom: 65px;
        @media (max-width: 1199px) {
            margin-bottom: 50px;
        }
        @media (max-width: 991px) {
            margin-bottom: 25px;
        }

        .grid {
            display: flex;
            @media (max-width: 767px) {
                flex-wrap: wrap;
            }

            .grid__item {
                float: none;
                @media (min-width: 1200px) {
                    padding-left: 44px;
                }
            }
        }

        .featurebox {
            height: 100%;
            text-align: center;
            padding: 30px 15px;
            border-radius: 7px;
            background-color: #F7F7F7;
            box-shadow: 3px 4px 7px 0 rgba(0, 0, 0, 0.25);
            @media (max-width: 767px) {
                height: unset;
                display: flex;
                text-align: left;
                padding-left: 19px;
                padding-right: 10px;
                align-items: center;
                margin-bottom: 15px;
                padding-top: 10px;
                padding-bottom: 10px;
                justify-content: flex-start;
            }
            @media (max-width: 575px) {
              padding-left: 19px;
            }

            .featurebox__img {
                max-height: 60px;
            }

            img {
                margin-bottom: 10px;
                @media (max-width: 767px) {
                  margin-bottom: 0;
                }
            }

            .featurebox__content {
              @media (max-width: 767px) {
                padding-left: 20px;
              }
            }
            
            .featurebox__title {
                line-height: 1;
                color: #363636;
                font-weight: 500;
                letter-spacing: 0;
                font-size: 18.14px;
                margin-bottom: 15px;
                text-transform: none;
                font-family: 'Poppins', sans-serif;
                @media (max-width: 767px) {
                    margin-bottom: 5px;
                }
                @media (max-width: 375px) {
                  font-size: 16px;
                }
            }

            p {
                font-weight: 300;
                margin-bottom: 0;
                color: #444444;
                line-height: 1.4;
                font-size: 18.14px;
                font-family: 'Poppins', sans-serif;
                @media (max-width: 1199px) {
                    font-size: 16px;
                }
                @media (max-width: 767px) {
                    font-size: 18.14px;
                }
                @media (max-width: 375px) {
                  font-size: 16px;
                }
            }
        }
    }

    .banner__sale {
        padding: 15px 0;
        text-align: center;
        background-size: cover;
        background-color: #C8A1B6;
        background-repeat: no-repeat;
        @media (min-width: 768px) {
          background: #C8A1B6 !important;
        }
        @media (max-width: 767px) {
          padding: 20px 0;
        }

        .banner__content {
          display: flex;
          margin-left: auto;
          margin-right: auto;
          align-items: center;
          flex-direction: column;
          justify-content: center;
        }

        h2 {
            padding: 0 10px;
            font-size: 38px;
            line-height: 1.4;
            color: #ffffff;
            font-weight: 400;
            margin-bottom: 5px;
            display: inline-block;
            text-transform: uppercase;
            font-family: 'Poppins', sans-serif;
            background-color: rgba(91, 91, 91, 0.71);
            @media (max-width: 991px) {
                font-size: 32px;
            }
            @media (max-width: 767px) {
                font-size: 26px;
            }
        }

        h4 {
            padding: 0 10px;
            font-size: 28px;
            font-weight: 300;
            color: #ffffff;
            letter-spacing: 0;
            margin-bottom: 0;
            text-transform: none;
            display: inline-block;
            font-family: 'Poppins', sans-serif;
            background-color: rgba(91, 91, 91, 0.71);
            @media (max-width: 991px) {
                font-size: 24px;
            }
            @media (max-width: 767px) {
                font-size: 20px;
            }
        }
    }

    .collection__list {
        padding-top: 15px;

        .block__title {
            font-size: 26px;
            line-height: 1.3;
            font-weight: 400;
            color: #363636;
            text-align: center;
            margin-bottom: 20px;
            text-transform: none;
            font-family: 'Poppins', sans-serif;
            @media (max-width: 767px) {
              font-size: 24px;
            }
        }
    } 

    .iconbox {
      padding: 15px 0;
      margin-top: 50px;
      background-color: #C8A1B6;
      @media (max-width: 768px) {
        padding-bottom: 5px;
      }

      .grid {
        display: flex;
        flex-wrap: wrap;
      }

      .small--one-half {
        @media only screen and (max-width: 768px) {
          flex-basis: 50%;
          max-width: 50%;
        }
      }

      .medium-up--one-fifth {
        @media only screen and (min-width: 769px) {
          flex-basis: 25%;
          max-width: 25%;
        }
      }

      .iconbox__item {
        text-align: center;
        @media (max-width: 768px) {
          margin-bottom: 20px;
        }

        img {
          margin-bottom: 5px;
        }

        .iconbox__title {
          color: #fff;
          font-weight: 400;
          letter-spacing: 0;
          font-size: 18.14px;
          text-transform: none;
          font-family: 'Poppins', sans-serif;
        }
      }
    }
}


.selectbox--print {
  max-width: 100%;

  .selectbox__print-box {
    position: relative;
  }

  .selectbox__voidpicker {
    display: none; 
  }

  .selectbox__print-wrapper ul {
    margin-bottom: 0;
    margin-left: 0;
    padding-left: 0px;
  }
  
  .selectbox__print-wrapper ul li { 
    &:first-child {
      display: none;
    }
  }

  .selectbox__print-wrapper ul li {
    display: flex;
    cursor: pointer;
    list-style: none;
    padding: 8px 5px;
    color: #000000;
    margin-bottom: 0;
    font-size: 13.14px;
    align-items: center;
    font-family: 'Poppins', sans-serif;
    border-bottom: 1px solid #818181;
  }
  
  .selectbox__print-wrapper ul li:hover {
    background: #F3F3F3;
  }
  
  .selectbox__print-wrapper ul li img,.selectbox__btn-select li img {
    width: 25px;
    height: 25px;
    object-fit: cover;
    margin-right: 6px;
  }
  
  .selectbox__print-wrapper ul li span {
    color: #000000;
    font-size: 13.14px;
    font-family: 'Poppins', sans-serif;
  }
  
  /* item list */
  
  .selectbox__print-wrapper {
    z-index: 3;
    top: 100%;
    width: 100%;
    display: none;
    position: absolute;
    border-left: 1px solid #818181;
    border-right: 1px solid #818181;
    background-color: #fff;
  }
  
  .open {
    display: show !important;
  }
  
  .selectbox__btn-select {
    width: 100%;
    height: 35px;
    cursor: pointer;
    border-radius: 0;
    text-align: left;
    padding: 3px 15px;
    position: relative;
    background-color: #fff;
    border: 1px solid #818181;
    font-family: 'Poppins', sans-serif;
    font-size:14px;

    &:after {
      content: '';
      width: 0; 
      height: 0; 
      top: 50%;
      right: 10px;
      position: absolute;
      transform: translateY(-50%);
      border-top: 5px solid #000;
      border-left: 5px solid transparent;
      border-right: 5px solid transparent;
    }
  }

  .selectbox__btn-select li {
    list-style: none;
    margin-bottom: 0;
    font-weight: 300;
    padding-bottom: 0px;
    display: flex;
    align-items: center;
    font-family: 'Poppins', sans-serif;
  }
  
  .selectbox__btn-select:hover li { 
    margin-left: 0px;
  }
  
  .selectbox__btn-select:focus {
    outline:none;
  }
  
  .lang-select{
    margin-left: 50px;
  }


  .selectbox__btn-select img[src=""],
  .selectbox__print-wrapper ul li img[src=""],
  .selectbox__btn-select img[src="undefined"],
  .selectbox__print-wrapper ul li img[src="undefined"] {
    display: none;
  }
  
}

.countdown {
  bottom: 0;
  width: 100%;
  display: none;
  position: fixed;
  z-index: 9999999999;
  background-color: #c8a1b6;
  @media (max-width: 767px) {
    display: block;
  }
  
  .hasCountdown {
    width: 100%;
    max-width: 400px;
    margin-left: auto;
    margin-right: auto;
    padding: 10px 30px;
  }

  .countdown_row {
    display: flex;
    margin-left: -15px;
    margin-right: -15px;
    justify-content: center;

    .countdown_section {
      color: #fff;
      padding: 0 15px;
      flex-basis: 25%;
      font-weight: 400;
      position: relative;
      font-size: 18.14px;
      text-align: center;
      letter-spacing: 1.5px;
      text-transform: uppercase;
      font-family: Futura,sans-serif;
     
      .countdown_amount {
        color: #fff;
        font-size: 20px;
        font-weight: 800;
      }
    }

    .countdown_section + .countdown_section {
      &:before {
        content: ':';
        left: 0;
        font-size: 38px;
        font-weight: 600; 
        position: absolute;
        font-family: 'Poppins', sans-serif;
      }
    }
  }
}


