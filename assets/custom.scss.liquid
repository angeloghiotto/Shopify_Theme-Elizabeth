@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap');

.hidden{
  display: none !important;
}


.buy2free1{
  &.js-drawer-open{
    overflow: auto;
  }
  
  #CartDrawer{
    display: none !important;
  }
  
  .main-content:after{
    display: none !important;
  }
}


.buy2free1_landing_template{
  display: flex;
  align-items: center;
  justify-content: center;

  .template_inner{
    max-width: 320px;
    background: white;
    color: white;
    padding: 0px;
    border-radius: 0px;
  }
      
  .landing-header{
    background: #C89FB6;
    text-align: center;
    position: relative;
    line-height: 0;
    
    .header-banner{
      position: relative;
    }
    
    .header-banner-mask{
      
      content: '';
      position: absolute;
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.25);
      
    }
	
    p{
      position: absolute;
      left: 0;
      bottom: 0;
      right: 0;
      color: white;
      font-weight: bold;
      font-size: 18px;
      line-height: 1.2;
      padding-left: 20px;
      padding-right: 20px;
      font-weight: normal;
    }
  }

  .landing-body{
    background: #FFEFD3;
    text-align: center;
    padding-top: 50px;
    padding-bottom: 50px;
    position: relative;
    

    .text1{
      color: black;
      font-weight: bold;
      font-size: 20px;
      margin-bottom: 0px;
    }

    .stamped-starrating{
      .stamped-fa-star{
        color: black !important;

        &:before{
          font-size: 22px;
        }
      }
    }

    .text2{
      color: black;
      margin-top: 10px;
      font-size: 12px;
    }
    
    .price-row{
      display: flex;
      justify-content: center;
      align-items: center;
      color: black;
      font-weight: bold;
      margin-bottom: 25px;
      
      .original_price{
        margin-left: 5px;
        margin-right: 5px;
        text-decoration: line-through;
        text-decoration-thickness: 2px;
      }
      
      .discount_price{
        margin-left: 5px;
        margin-right: 5px;
      }
    }

    .btn-start{
      background: black;
      color: white;
      padding: 10px 15px;
      border-radius: 8px;
      font-weight: bold;
      font-size: 16px;
      display: inline-block;
      margin-bottom: 20px;
      text-transform: uppercase;
      width: calc(100% - 50px);
    }

  }

  .landing-footer{
    text-align: center;
    padding-bottom: 50px;

    .stamped-starrating{
      margin-top: 30px !important;
      display: inline-block;
      margin-right: 0px !important;

      .stamped-fa-star{
        color: #eebc18 !important;

        &:before{
          font-size: 40px;
        }
      }
    }

    .text3{
      color: black;
      font-weight: bold;
      margin-top: 10px;
      font-size: 20px;
    }
    
    .footer-image-wrap{
      max-width: 230px;
      margin: 0 auto;
      margin-top: 20px;
      
      img{
        box-shadow: 12px 12px #fdc2c2;
        border-radius: 16px;
      }
    }
    
  }

}
  

.buy2free1_template{
  #shopify-section-buy2free1-template-header{
    padding-top: 0px;
    padding-bottom: 0px;
    
    position: sticky;
    top: 0px;
    z-index: 20;
    background: white;
    padding-bottom: 15px;
    box-shadow: 0px -1px 5px rgba(0,0,0,0.1);
    
    .template-header-top{
      background: #FFEFD3;
      padding-top: 10px;
      padding-bottom: 10px;
      
      .header-title{
        color: black;
        font-size: 16px;
        text-transform: none;
        font-weight: bold;
      }

      .header-description{
        text-transform: uppercase;
        font-size: 20px;
        margin-top: 10px;
        font-weight: bold;
        color: black;
        margin-bottom: 0px;
        
        colored{
          color: #07A161;
        }
      }
    }
    
    .header-progress{
      display: flex;
      justify-content: center;
      margin-top: 15px;
      margin-bottom: 5px;
      padding-left: 15px;
      padding-right: 15px;
      
      .header-progress-item{
        width: 100px;
        height: 100px;
        border-radius: 5px;
        background-image: url("https://cdn.shopify.com/s/files/1/0350/5061/9020/files/discount_progress_default.png?v=1615521651");
        background-size: cover;
        background-position: center;
        margin-left: 15px;
        margin-right: 15px;
        position: relative;
        text-align: center;
        border: 2px solid #CCCCCC;
        
        @media screen and (max-width: 480px){
          margin-left: 5px;
          margin-right: 5px;
        }
        
        
        .header-progress-item-image{
          border-radius: 5px;
          width: 100%;
          height: 100%;
          display: block;
          visibility: hidden;
        }
        
        .animation-wrapper{
          position: absolute;
          left: 0;
          top: 0;
          width: 100%;
          height: 100%;
          z-index: 1;
          display: none;
          background: white;
          border-radius: 6px;
        }
        
        &.animation-start{
          
          .animation-wrapper{
            display: block;
          }
        }
        
        .header-progress-item_btn_remove{
          position: absolute;
          left: 0px;
          top: 0px;
          align-items: center;
          justify-content: center;
          background: black;
          border-radius: 50%;
          color: white;
          width: 22px;
          height: 22px;
          font-size: 13px;
          line-height: 1;
          transform: translate3d(-50%, -50%, 0px);
          cursor: pointer;
          user-select: none;
          
          display: none;
          z-index: 2;
        }
        
        
        &.set{
          border-color: black;
          .header-progress-item-image{
            visibility: visible;
          }
          
          
          .header-progress-item_btn_remove{
            display: flex;
          }
        }
        
        &.active{
          border-color: #07A161;
        }
        
        .item-num{
          font-size: 12px;
        }
        
      }
    }
    
    .header-progress-color{
      .header-progress-color-content{
        width: 120px;
        height: 30px;
        border-radius: 8px;
        display: inline-block;
        margin-top: 10px;
        margin-bottom: 10px;
      }
    }
    
    
    
    .header-footer{
      border-top: 2px solid #CBC9C9;
      margin-top: 35px;
      
      h3{
        display: block;
        text-align: center;
        margin-top: 20px;
        font-weight: bold;
      }
    }
  }
  
  
  .product-single__meta{
    .product-single__form{
      .product__quantity{
        display: none !important;
      }
    }
  }
  
  
  
  
  
  .grid-product{
    .grid-product__content{
      .quick-product__btn{
        display: block !important;
        opacity: 1 !important;
        
        width: auto;
        right: 10px;
        left: auto;
        border-radius: 30px;
        background: #C5C4C5;
        color: black;
        padding: 5px 10px;
        transform: none !important;
        
        .quick-product__label{
          font-size: 0px;
          &:after{
            content: 'View';
            font-size: 12px;
            text-transform: uppercase;
          }
        }
      }
    }
    
    .product-grid-item_form_wrapper{
      form.product-single__form{
        text-align: center;
        
        .variant__label{
          display: none;
        }
        
        .product__quantity{
          display: none;
        }
        
        button{
          max-width: 170px;
          width: 100%;
          border-radius: 40px;
          color: white;
          background: #BD0404;
          font-size: 12px;
          padding: 13px 5px;
          
          &[disabled]{
            background: #f6f6f6 !important;
            
            /*
            &:hover, &:active{
              background: #f6f6f6 !important;
            }
            */
          }
        }
      }
