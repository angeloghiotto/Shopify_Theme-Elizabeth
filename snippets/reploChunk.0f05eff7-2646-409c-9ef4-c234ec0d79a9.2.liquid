<script src="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/js/splide.min.js"></script>
<link href="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/css/splide.min.css" rel="stylesheet"></link>

<script>
    let products = [
        { slug: 'horn-to-be-wild-ruffle-romper', thumbnail: 'https://cdn.shopify.com/s/files/1/0350/5061/9020/files/Horn_To_Be_Wild_8cf1dcd7-1cf3-480d-8d1b-2ad35ea4688e_100x.jpg?v=1675056737' },
        { slug: 'pink-scooter-ruffle-romper', thumbnail: 'https://cdn.shopify.com/s/files/1/0350/5061/9020/files/Untitled_design_18_61bdedd2-6973-4082-90be-385b014ebcbb_100x.png?v=1675057068' },
        { slug: 'once-and-flor-al-romper', thumbnail: 'https://cdn.shopify.com/s/files/1/0350/5061/9020/files/9a8a6a3d-untitled-design-3_101c01c000000000000000.png?v=1674542416' },
        { slug: 'zebra-neon-ruffle-romper', thumbnail: 'https://cdn.shopify.com/s/files/1/0350/5061/9020/files/Born_To_Stand_Out_d3858e31-0c8b-4e05-b804-9d3383577f52_100x.jpg?v=1675110916' },
        { slug: 'rainbow-ruffle-romper', thumbnail: 'https://cdn.shopify.com/s/files/1/0350/5061/9020/files/Color_My_World_6fe86da7-049c-43a4-a8d1-5ce21e74cfcf_100x.jpg?v=1675057217' },
    ]
    const discountCode = 'BUY2GET1'

    const wrapper = document.currentScript.parentElement.querySelector('.bundle-creator')
    wrapper.id = `bundle-${(Math.random() + '').slice(-6)}`

    const checkSvg = `<svg width="10" height="8" viewBox="0 0 10 8" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M9 1.5L3.5 7L1 4.5" stroke="white" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>`

    Promise.all(products.map(({ slug }) =>
        fetch(`${window.Shopify.routes.root}products/${slug}.js`)
            .then((response) => response.json())
    )).then(results => {
        products = results.map((result, i) => ({ ...result, ...products[i] }))
        let selectedProduct = products[0]
        let selectedVariant = selectedProduct.variants[0]
        const bundle = {}

        products.forEach(product => product.images.forEach(url => {
            const link = document.createElement("link");
            link.rel = "preload";
            link.as = "image";
            link.href = url
            document.head.appendChild(link);
        }))

        const renderBelowColors = () => {
            const belowColorsWrapper = wrapper.querySelector('.below-colors')
            const inBundle = Object.values(bundle).filter(e => e.quantity)
            const totalInBundle = inBundle.map(e => e.quantity).reduce((acc, curr) => acc + curr, 0)
            belowColorsWrapper.innerHTML = `
                <div class='bundle-section'>
                    <p>PICK YOUR SIZE</p>
                    <div class="size-selector">
                        ${selectedProduct.variants.map(variant => `
                            <button class="variant-option ${variant.id === selectedVariant.id? 'selected' : ''}">${variant.public_title}</button>
                        `).join('')}
                    </div>
                </div>
                <button class="pink-button add-to-bundle">$${selectedProduct.price / 100} | Add To Bundle</button>
                ${inBundle.length > 0 ? `<div class="bundle">
                    <div class="bundle-title">Third Product FREE! Add 3 products to the cart and pay just for 2</div>
                    <div class="bundle-progress">
                        <div class="bundle-step bundle-step-active"><div class="bundle-check">${checkSvg}</div><span>1 Romper</span></div>
                        <div class="bundle-step ${totalInBundle > 1? 'bundle-step-active': ''}"><div class="bundle-check">${checkSvg}</div><span>2 Rompers</span></div>
                        <div class="bundle-step ${totalInBundle > 2? 'bundle-step-active': ''}"><div class="bundle-check">${checkSvg}</div><span> Free romper</span></div>
                    </div>
                    <div class="bundle-items">
                        ${inBundle.map(({ quantity, product, variant }) => `<div class="bundle-item">
                            <img class="color" src="${product.thumbnail}" />
                            <select class="quantity-selector" data-id="${product.id}-${variant.id}">
                                ${new Array(9).fill(null).map((_, i) => `<option value="${i + 1}" ${quantity === i + 1? 'selected': ''}>x${i + 1}</option>`).join('')}
                                <option value="remove">Remove</option>
                            </select>
                            <div>
                                <strong>${product.title}</strong>
                                <div style="font-size: 16px">${variant.public_title}</div>
                            </div>
                        </div>`).join('')}
                    </div>
                    <button class="pink-button add-to-cart">Add To Cart</button>
                </div>`: ''}
            `
            
            ;[...belowColorsWrapper.querySelectorAll('.quantity-selector')].forEach((node, i) =>
                node.addEventListener('change', (event) => {
                    if (event.target.value === 'remove')
                        delete bundle[event.target.dataset.id]
                    else
                        bundle[event.target.dataset.id].quantity = parseInt(event.target.value)
                    renderBelowColors()
                }))

            ;[...belowColorsWrapper.querySelectorAll('.variant-option')].forEach((node, i) =>
                node.addEventListener('click', () => {
                    selectedVariant = selectedProduct.variants[i]
                    renderBelowColors()
                }))

            belowColorsWrapper.querySelector('.add-to-bundle').addEventListener('click', () => {
                const key = `${selectedProduct.id}-${selectedVariant.id}`
                bundle[key] = bundle[key] || { product: selectedProduct, variant: selectedVariant, quantity: 0 }
                bundle[key].quantity += 1
                renderBelowColors()
                belowColorsWrapper.querySelector('.bundle').scrollIntoView({behavior: "smooth", block: "nearest", inline: "nearest"})
            })

            const addToCartButton = belowColorsWrapper.querySelector('.add-to-cart')
            if (addToCartButton)
                addToCartButton.addEventListener('click', () => {
                    fetch(`${window.Shopify.routes.root}cart/add.js`, {
                        method: 'POST',
                          headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({
                                items: inBundle.map(({ quantity, product, variant }) => (
                                    {
                                        quantity: quantity,
                                        id: variant.id,
                                    }
                                ))
                            })
                    }).then(() => window.location = `${window.Shopify.routes.root}checkout?discount=${discountCode}`)
                    .catch(window.alert)
                })
        }

        const render = () => {
            wrapper.innerHTML = `
                <div class="creator-inner-wrapper">
                    <div style="max-width: 530px;min-width: 300px; width: 100%;">
                        ${['carousel', 'carousel-thumbnails'].map(id => `<section class="splide ${id}">
                            <div class="splide__track">
                                    <ul class="splide__list">
                                        ${selectedProduct.images.map(url => `<li class="splide__slide"><img src="${url}" style="display:block;padding:0;margin:0;--rmh:100%;object-fit:contain;width:100%;min-height:var(--rmh, auto);max-height:var(--rmh, none)"></li>`).join('')}
                                    </ul>
                            </div>
                        </section>`).join('')}
                    </div>
                    <div style="flex-basis: 530px">
                        <h3>Bums N' Roses Ruffle Footie</h3>
                        ${product.title}
                        <div class='bundle-section'>
                            <p>PICK YOUR LIMITED EDITION PRINT</p>
                            <div class="color-selector">
                                ${products.map(product => `
                                    <img tabindex="0" class="color-option ${product.id === selectedProduct.id? 'selected' : ''}" src="${product.thumbnail}" />
                                `).join('')}
                            </div>
                        </div>
                        <div class="below-colors"></div>
                    </div>
                </div>
            `

            renderBelowColors()

            ;[...wrapper.querySelectorAll('.color-option')].forEach((node, i) =>
                node.addEventListener('click', () => {
                    selectedProduct = products[i]
                    selectedVariant = selectedProduct.variants.filter(v => v.public_title === selectedVariant.public_title)[0] || selectedProduct.variants[0]
                    render()
                }))

            const carousel = new Splide(`#${wrapper.id} .carousel`, {
                type: 'fade',
                rewind: true,
                pagination: false,
                arrows: false,
            })
            const thumbnails = new Splide(`#${wrapper.id} .carousel-thumbnails`, {
                rewind: true,
                pagination: false,
                isNavigation: true,
                arrows: false,
            })

            carousel.sync(thumbnails)
            carousel.mount()
            thumbnails.mount()
        }

        render({ initSplide: true })
    })
</script>

